pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                // Étape de construction de l'application
                echo 'build the app..'
            }
        }
        
        stage('Test') {
            steps {
                // Étape de test de l'application
                echo 'test th app ..'
            }
        }
        
        stage('Deploy') {
            steps {
                // Étape de déploiement de l'application (peut varier en fonction de l'environnement)
                echo 'deploy the app..'
            }
        }
    }
    
